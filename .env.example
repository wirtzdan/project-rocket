NODE_ENV=
NEXT_PUBLIC_ADMIN_USER_IDS=

# Convex Configuration
# Get your deployment URL by running: npx convex dev
NEXT_PUBLIC_CONVEX_URL=
# Note: OUTSETA_SUBDOMAIN must be set in the Convex dashboard (Settings > Environment Variables), not here.
# It should match your Outseta subdomain (e.g., "project-rocket" for project-rocket.outseta.com).

# Outseta API Configuration
# Generate API keys at: Settings > Integrations > API Keys
# Format: "api-key:secret-key" (server-side only, never expose to client)
OUTSETA_API_KEY=

# PostHog Configuration
# Get your PostHog API key from: https://app.posthog.com/project/settings
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://eu.i.posthog.com