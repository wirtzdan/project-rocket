{
  "name": "project-starter-chakra",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "generate:favicons": "tsx scripts/generate-favicons.tsx",
    "generate:logo": "tsx scripts/generate-logo-svg.tsx",
    "generate:outseta-css": "tsx scripts/generate-outseta-css.ts",
    "prebuild": "pnpm generate:logo && pnpm generate:favicons && pnpm generate:outseta-css",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "ultracite check",
    "check:fix": "biome check --write .",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.32.0",
    "@emotion/react": "^11.14.0",
    "@types/node": "^22.13.0",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "convex": "^1.31.7",
    "jose": "^6.1.3",
    "jwt-decode": "^4.0.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.342.1",
    "react": "19.2.4",
    "react-canvas-confetti": "^2.0.7",
    "react-dom": "19.2.4",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "typescript": "^5.9.3",
    "vanilla-cookieconsent": "^3.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@chakra-ui/cli": "^3.32.0",
    "png-to-ico": "^2.1.3",
    "sharp": "^0.33.5",
    "tsx": "^4.21.0",
    "ultracite": "7.1.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.13",
      "@types/react-dom": "19.2.3"
    }
  }
}
